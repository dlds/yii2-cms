{
    "name": "dlds/yii2-cms",
    "description": "Easy CMS system build on Yii2",
    "keywords": ["yii2", "cms", "blog"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/dlds/yii2-cms/issues",
        "wiki": "https://github.com/dlds/yii2-cms/wiki",
        "source": "https://github.com/dlds/yii2-cms"
    },
    "authors": [
        {
            "name": "Jiri Svoboda",
            "email": "jiri.svoboda@dlds.cz"
        }
    ],
    "require": {
        "yiisoft/yii2": "*"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "autoload": {
        "psr-4": {
            "dlds\\cms\\": "src"
        }
    }
}